local internal = {...}
local lib = {}
local conf = {}
function lib.get(x,y)
    local fp = "/ect/"..string.gsub(x,".","/")..".conf"
    local file = internal.fs.open(fp,"r")
    local data = file.readAll()
    file.close()
    if data == nil then data = "nil" end
    return load("return "..data)()
end
function lib.set()
return lib,"conf"